#!/usr/bin/env python2

import sys

def main():
    f = open("szoveg")

    for line in f:
        for c in line:
            if c == "3":
                sys.stdout.write('E')
            elif c == "4":
                sys.stdout.write('A')
            elif c == "1":
                sys.stdout.write('I')
            elif c == "7":
                sys.stdout.write('T')
            elif c == "5":
                sys.stdout.write('S')
            elif c == "0":
                sys.stdout.write('O')
            else:
                sys.stdout.write(c)
    f.close()

if __name__ == "__main__":
    main();
